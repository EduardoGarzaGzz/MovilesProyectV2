<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:app = "http://schemas.android.com/apk/res-auto"
	xmlns:tools = "http://schemas.android.com/tools"
	android:layout_width = "match_parent"
	android:layout_height = "match_parent"
	tools:context = ".ui.fragments.RegistrarCitsaFragment"><android.support.constraint.ConstraintLayout
	android:layout_width = "match_parent"
	android:layout_height = "match_parent">

<TextView
	android:id = "@+id/textView2"
	android:layout_width = "wrap_content"
	android:layout_height = "wrap_content"
	android:layout_marginStart = "8dp"
	android:layout_marginTop = "8dp"
	android:layout_marginEnd = "8dp"
	android:text = "Agregar nueva cita"
	android:textSize = "24sp"
	android:textStyle = "bold"
	app:layout_constraintEnd_toEndOf = "parent"
	app:layout_constraintStart_toStartOf = "parent"
	app:layout_constraintTop_toTopOf = "parent" /><EditText
	android:id = "@+id/editText2"
	android:layout_width = "0dp"
	android:layout_height = "wrap_content"
	android:layout_marginStart = "8dp"
	android:layout_marginTop = "32dp"
	android:layout_marginEnd = "8dp"
	android:ems = "10"
	android:hint = "Titulo"
	android:inputType = "textPersonName"
	app:layout_constraintEnd_toEndOf = "parent"
	app:layout_constraintStart_toStartOf = "parent"
	app:layout_constraintTop_toBottomOf = "@+id/textView2" /><EditText
	android:id = "@+id/editText3"
	android:layout_width = "0dp"
	android:layout_height = "80dp"
	android:layout_marginStart = "8dp"
	android:layout_marginTop = "8dp"
	android:layout_marginEnd = "8dp"
	android:ems = "10"
	android:gravity = "start|top"
	android:hint = "Descripcion"
	android:inputType = "textMultiLine"
	app:layout_constraintEnd_toEndOf = "parent"
	app:layout_constraintStart_toStartOf = "parent"
	app:layout_constraintTop_toBottomOf = "@+id/editText2" /><com.google.android.gms.maps.MapView
	android:id = "@+id/mv_registrar_citas_maps"
	android:layout_width = "0dp"
	android:layout_height = "202dp"
	android:layout_marginStart = "8dp"
	android:layout_marginTop = "8dp"
	android:layout_marginEnd = "8dp"
	app:layout_constraintEnd_toEndOf = "parent"
	app:layout_constraintStart_toStartOf = "parent"
	app:layout_constraintTop_toBottomOf = "@+id/editText3" /><LinearLayout
	android:id = "@+id/linearLayout"
	android:layout_width = "0dp"
	android:layout_height = "wrap_content"
	android:layout_marginStart = "8dp"
	android:layout_marginTop = "8dp"
	android:layout_marginEnd = "8dp"
	android:orientation = "horizontal"
	app:layout_constraintEnd_toEndOf = "parent"
	app:layout_constraintStart_toStartOf = "parent"
	app:layout_constraintTop_toBottomOf = "@+id/mv_registrar_citas_maps">
            <TextView
				android:id = "@+id/textView3"
				android:layout_width = "wrap_content"
				android:layout_height = "wrap_content"
				android:layout_weight = "1"
				android:text = "Log:" /><TextView
	android:id = "@+id/textView4"
	android:layout_width = "wrap_content"
	android:layout_height = "wrap_content"
	android:layout_weight = "1"
	android:text = "Lat:" /></LinearLayout><Button
	android:id = "@+id/button"
	android:layout_width = "0dp"
	android:layout_height = "wrap_content"
	android:layout_marginStart = "8dp"
	android:layout_marginTop = "8dp"
	android:layout_marginEnd = "8dp"
	android:text = "Agregar"
	app:layout_constraintEnd_toEndOf = "parent"
	app:layout_constraintStart_toStartOf = "parent"
	app:layout_constraintTop_toBottomOf = "@+id/linearLayout" /><Button
	android:id = "@+id/button2"
	android:layout_width = "0dp"
	android:layout_height = "wrap_content"
	android:layout_marginStart = "8dp"
	android:layout_marginTop = "8dp"
	android:layout_marginEnd = "8dp"
	android:text = "Cancelar"
	app:layout_constraintEnd_toEndOf = "parent"
	app:layout_constraintStart_toStartOf = "parent"
	app:layout_constraintTop_toBottomOf = "@+id/button" /></android.support.constraint.ConstraintLayout></FrameLayout>